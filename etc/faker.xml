<?xml version="1.0" encoding="UTF-8"?>
<fakers-wrap>
    <fakers>
        <faker>
            <type>log</type>
            <interval>1000</interval>
            <level>info</level>
            <messages>
                <message>Processing message</message>
                <message>Processing finished</message>
                <message>Evaluating critical boundaries</message>
                <message>Searching for useful data</message>
                <message>Search complete</message>
            </messages>
        </faker>
        <faker>
            <type>log</type>
            <!-- time in ms -->
            <interval>2000</interval>
            <level>warn</level>
            <messages>
                <message>server no response</message>
                <message>dangerous action</message>
                <message>no user selected!</message>
            </messages>
        </faker>
        <faker>
            <type>log</type>
            <interval>5000</interval>
            <level>error</level>
            <exceptions>
                <exception>exception</exception>
                <exception>io</exception>
                <exception>null</exception>
            </exceptions>
        </faker>
    </fakers>
</fakers-wrap>